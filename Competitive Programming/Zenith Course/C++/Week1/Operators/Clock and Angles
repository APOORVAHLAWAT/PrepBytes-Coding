#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
    
    int t;
    cin>>t;
    while(t--)
    {
      int hour,minutes;
      cin>>hour>>minutes;
      float h = (hour%12 + (float)minutes/60)*30;   //360/12
      float m = minutes*6;  // 360/60
      float angle = fabs(h-m);
      if(angle > 180) angle = 360-angle;
      cout<<angle<<endl;
      
    }
    
    //write your code here
    
    return 0;
  }
