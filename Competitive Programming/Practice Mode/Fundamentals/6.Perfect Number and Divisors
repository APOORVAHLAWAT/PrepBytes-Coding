#include <bits/stdc++.h>
using namespace std;

int main()
{
  
  int t;
  cin >> t;
  
  while(t--)
  {
    int num,divi,sum=0;
    
    cin >> num;
    
    for(int i=1; i < num; i++)
    {
      divi = num % i;
      if(divi == 0)
      {
        sum  += i;
      }
    }
    
    if(sum == num)
    {
      cout << "true" << endl;
    }
    else
    {
      cout << "false" << endl;
    }
  }
  //write your code here
  
  return 0;
}
